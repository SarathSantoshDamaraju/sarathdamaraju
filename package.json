{
  "name": "portfolio",
  "version": "0.3.0",
  "private": true,
  "engines": {
    "node": ">=10.16"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "pretty": "prettier --write src/**/*.js",
    "lint:js": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix"
  },
  "lint-staged": {
    "*.js": [
      "yarn pretty",
      "yarn lint:js",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@mdx-js/loader": "^1.4.0",
    "@next/mdx": "^9.0.5",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "dayjs": "^1.8.28",
    "gray-matter": "^4.0.2",
    "next": "^9.3.0",
    "node-glob": "^1.2.0",
    "node-sass": "^4.14.1",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "raw-loader": "^4.0.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-markdown": "^4.3.1",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "frontmatter-markdown-loader": "^3.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5"
  }
}
